<?xml version="1.0" encoding="UTF-8"?>
<slot-configurations xmlns="http://www.demandware.com/xml/impex/slot/2008-09-08">
    <slot-configuration slot-id="minicart-banner" context="global" configuration-id="10%offorderover$150" assigned-to-site="true">
        <description>Free Ground Shipping for Orders over $150</description>
        <template>slots/html/htmlslotcontainer.isml</template>
        <enabled-flag>true</enabled-flag>
        <schedule>
            <recurrence>
                <time-of-day>
                    <time-from>21:00:00.000Z</time-from>
                    <time-to>08:59:00.000Z</time-to>
                </time-of-day>
            </recurrence>
        </schedule>
        <content>
            <html>
                <body xml:lang="x-default">&lt;div class="minicartslot"&gt;
                    &lt;p style="padding: 0px 10px;"&gt;10% off any order with a purchase of over $150&lt;/p&gt;
                    &lt;/div&gt;</body>
            </html>
        </content>
    </slot-configuration>

