    <content content-id="checkout-security-code">
        <display-name xml:lang="x-default">Security Code Tooltip</display-name>
        <display-name xml:lang="fr-FR">Infobulle sur le code de sécurité</display-name>
        <display-name xml:lang="it-IT">Suggerimento per codice di sicurezza</display-name>
